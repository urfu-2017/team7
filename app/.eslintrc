{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": ["error", "never"],
    "indent": ["error", 4],
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props" : "off",
    "no-plusplus": "off",
    "react/prefer-stateless-function": [0],
    "react/prop-types": [0]
  }
}
