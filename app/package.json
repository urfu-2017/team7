{
  "name": "team7chat",
  "version": "1.0.0",
  "repository": "https://github.com/urfu-2017",
  "license": "MIT",
  "scripts": {
    "dev": "node .",
    "build": "next build",
    "start": "NODE_ENV=production node .",
    "mocha": "mocha --timeout 5000 --require babel-core/register test/*.spec.js",
    "mwatch": "npm run mocha -- --watch",
    "test": "npm run lint && npm run mocha",
    "stats": "mocha --no-timeouts --require babel-core/register test/*.stats.js",
    "lint": "eslint . --ext .js,.jsx",
    "deploy": "now --public -e NODE_ENV=production --token $NOW_TOKEN --npm",
    "alias": "now alias --token=$NOW_TOKEN"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^5.0.1",
    "express": "^4.14.0",
    "express-promise-router": "^3.0.1",
    "got": "^8.3.0",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "next": "^5.0.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "prop-types": "^15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router-dom": "^4.2.2",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/got": "^7.1.8",
    "@types/next": "^2.4.8",
    "@types/passport": "^0.4.4",
    "@types/passport-github": "^1.1.3",
    "@types/socket.io": "^1.4.32",
    "@types/socket.io-client": "^1.4.32",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
    "mocha": "^5.0.1",
    "now": "^10.2.1",
    "proxyquire": "^2.0.1",
    "supertest": "3.0.0"
  },
  "now": {
    "name": "team7chat",
    "alias": "team7chat"
  }
}
